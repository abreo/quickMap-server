<mapper namespace="org.quickmap.storageService.dao.base.FileInfoBaseMapper">


    <insert id="insertFileInfo"  useGeneratedKeys="true" keyProperty="id">
        INSERT INTO `file_info`
        (
        <trim suffixOverrides=",">
                    <if test="id!=null">
                        `id`,
                    </if>
                    <if test="filename!=null">
                        `filename`,
                    </if>
                    <if test="size!=null">
                        `size`,
                    </if>
                    <if test="path!=null">
                        `path`,
                    </if>
                    <if test="timestamp!=null">
                        `timestamp`,
                    </if>
                    <if test="author!=null">
                        `author`,
                    </if>
        </trim>
        )
        VALUES
        (
        <trim suffixOverrides=",">
                <if test="id!=null">
                    #{id},
                </if>
                <if test="filename!=null">
                    #{filename},
                </if>
                <if test="size!=null">
                    #{size},
                </if>
                <if test="path!=null">
                    #{path},
                </if>
                <if test="timestamp!=null">
                    #{timestamp},
                </if>
                <if test="author!=null">
                    #{author},
                </if>
        </trim>
        )
    </insert>




    <update id="updateFileInfo">
        UPDATE `file_info`
        SET
        <trim suffixOverrides=",">
            <if test="filename != null and filename!=''">
                `filename` = #{filename},
            </if>
            <if test="size != null">
                `size` = #{size},
            </if>
            <if test="path != null and path!=''">
                `path` = #{path},
            </if>
            <if test="timestamp != null">
                `timestamp` = #{timestamp},
            </if>
            <if test="author != null">
                `author` = #{author},
            </if>
        </trim>
        WHERE
        <trim suffixOverrides="and">
                `id` = #{id} and
        </trim>
    </update>



    <resultMap type="org.quickmap.storageService.dao.model.FileInfo" id="FileInfoMap">
